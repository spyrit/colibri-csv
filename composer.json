{
    "name": "spyrit/colibri-csv",
    "description": "Lightweight and performant CSV reader and writer library",
    "type": "library",
    "keywords": ["csv", "import", "export", "reader", "writer", "parser"],
    "authors": [
        {
            "name": "Charles SANQUER",
            "email": "charles.sanquer@gmail.com"
        }
    ],
    "license": "LGPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "ddeboer/transcoder": "1.0.*"
    },
    "suggest": {
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpunit/php-code-coverage": "^9.2",
        "symfony/stopwatch": "~2.4",
        "fzaninotto/faker": "~1.3",
        "doctrine/inflector": "^2.0",
        "symfony/phpunit-bridge": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "CSanquer\\ColibriCsv\\": "src/",
            "CSanquer\\ColibriCsv\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
